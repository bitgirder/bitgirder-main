# vim swap files
syntax: regexp
(^|.*/)\.[^/]+\.sw[a-z]$

# vim session file
syntax: regexp
^vim\.session$

# build dir
syntax: regexp
^build/

# go build files (this can be removed if/when we change build sys to build into
# build dir
syntax: regexp
.*/go/_test.*
.*/go/.*\.[6a]$
.*/6\.out$
.*/go/ident_parse_bench$

# hg serve dir
syntax: regexp
^.hg-serve/

# svn files when working in mixed checkouts
syntax: regexp
(^|/)\.svn(/|$)

# .env source file used by devs for their own particular workspace, if present
syntax: regexp
^\.env$
