{
    "$type": "bitgirder:ops:build:go@v1/GoProject",
    "direct-deps": [ "testing", "core", "io" ],
    "packages": [ 
        "mingle", 
        "mingle/parser/lexer", 
        "mingle/parser/loc",
        "mingle/parser/syntax",
        "mingle/types"
    ],
    "test-packages": [
        "mingle/testing",
        "mingle/testing/testval",
        "mingle/parser/testing"
    ],
    "test-commands": {
        "write-core-io-tests": {},
        "write-parser-tests": {},
        "write-reactor-tests": {},
        "check-core-io": {}
    },
    "test-data-generators": {
        "core-io-tests": {
            "$type": "bitgirder:ops:build:go@v1/GoCommandRun",
            "command": "write-core-io-tests",
            "argv": [ "--out-file", "${data_gen_dir}/core-io-tests.bin" ]
        },
        "core-parser-tests": {
            "$type": "bitgirder:ops:build:go@v1/GoCommandRun",
            "command": "write-parser-tests",
            "argv": [ "--out-file", "${data_gen_dir}/parser-tests.bin" ]
        },
        "reactor-seq-error-tests": {
            "$type": "bitgirder:ops:build:go@v1/GoCommandRun",
            "command": "write-reactor-tests",
            "argv": [ 
                "--out-file", "${data_gen_dir}/reactor-seq-error-tests.bin"
            ]
        }
    }
}
